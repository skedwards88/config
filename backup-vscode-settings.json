{
  "editor.renderWhitespace": "all",
  "files.autoSave": "afterDelay",
  "todohighlight.exclude": [
    "**/node_modules/**",
    "**/bower_components/**",
    "**/dist/**",
    "**/build/**",
    "**/.vscode/**",
    "**/.github/**",
    "**/_output/**",
    "**/*.min.*",
    "**/*.map",
    "**/.next/**"
  ],
  "todohighlight.include": [
    "**/*.md",
    "**/*.txt",
    "**/*.js",
    "**/*.jsx",
    "**/*.ts",
    "**/*.tsx",
    "**/*.html",
    "**/*.php",
    "**/*.css",
    "**/*.scss"
  ],
  "workbench.colorCustomizations": {
    "editor.selectionBackground": "#96c3ec",
    "editor.selectionHighlightBackground": "#5e666b",
    "editor.findMatchHighlightBackground": "#3d7a61",
    "editor.wordHighlightBackground": "#00ff9d"
  },
  "cSpell.userWords": [
    "Algolia",
    "AUTOTITLE",
    "cheatsheets",
    "codespace",
    "codespaces",
    "datacenter",
    "devops",
    "dotfiles",
    "endtip",
    "entrypoint",
    "frontmatter",
    "ghes",
    "hardcoded",
    "Janky",
    "Karabiner",
    "kilobranch",
    "Kubernetes",
    "megabranch",
    "Milli",
    "openapi",
    "orgs",
    "permalinks",
    "postgres",
    "Powerlevel",
    "prodname",
    "quickstart",
    "repos",
    "reusables",
    "scim",
    "screencasts",
    "scrollbar",
    "skedwards",
    "submodules",
    "symlinked",
    "templated",
    "thom",
    "Unprocessable",
    "webhook",
    "Webhooks",
    "workspace's",
    "zshscripts"
  ],
  "diffEditor.renderSideBySide": false,
  "code-runner.runInTerminal": true,
  "todohighlight.defaultStyle": {
    "backgroundColor": "#F06292",
    "color": "white"
  },
  "todohighlight.isCaseSensitive": false,
  "todohighlight.keywords": [

    "todocs", "todo", "fixme"],
  "editor.tabSize": 2,
  // "editor.acceptSuggestionOnEnter": "off",
  "workbench.editor.tabSizing": "shrink",
  "workbench.editor.tabCloseButton": "off",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "editor.renderWhitespace": "boundary",
  "search.mode": "newEditor",
  "diffEditor.renderSideBySide": false,
  "editor.minimap.showSlider": "always",
  "editor.minimap.renderCharacters": false,
  // "editor.rulers": [
  //     80
  //   ],
  "editor.selectionHighlight": false,
  "editor.renderLineHighlight": "all",
  "workbench.editor.restoreViewState": false,
  "editor.occurrencesHighlight": "off",
  //   "editor.wordWrap": "bounded",
  //   "[markdown]": {
  //     "editor.wordWrap": "bounded",
  //     "editor.quickSuggestions": true,
  //   },
  "explorer.confirmDragAndDrop": false,
  "search.exclude": {
    "**/translations": true
  },
  "search.searchEditor.reusePriorSearchConfiguration": true,
  "search.searchEditor.defaultNumberOfContextLines": 1,
  "markdownlint.config": {
    "MD004": false,
    "MD024": false,
    "MD034": false,
    "MD037": false,
    "MD040": false,
    "MD041": false
  },
  "editor.wordWrap": "on",
  "editor.autoClosingBrackets": "always",
  "editor.autoClosingQuotes": "always",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "markdown.extension.toc.updateOnSave": false,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "gitlens.advanced.blame.delayAfterEdit": 0,
  "gitlens.advanced.fileHistoryFollowsRenames": true,
  "gitlens.blame.avatars": false,
  "markdown.extension.italic.indicator": "_",
  "markdown.extension.orderedList.autoRenumber": false,
  "markdown.extension.orderedList.marker": "one",
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": false,
    "scminput": false,
    "yaml": true
  },
  "workbench.startupEditor": "none",
  // "[jsonc]": {
  //     "editor.defaultFormatter": "vscode.json-language-features"
  // },
  // "[html]": {
  //     "editor.defaultFormatter": "vscode.html-language-features"
  // },
  // "[javascript]": {
  //     "editor.defaultFormatter": "vscode.typescript-language-features"
  // },
  // "[yaml]": {
  //     "editor.defaultFormatter": "redhat.vscode-yaml"
  // },
  // "gitlens.hovers.currentLine.over": "line",
  // "workbench.editorAssociations": [
  //     {
  //         "viewType": "jupyter.notebook.ipynb",
  //         "filenamePattern": "*.ipynb"
  //     }
  // ],
  // "workbench.startupEditor": "newUntitledFile",
  // "cSpell.dictionaryDefinitions": [],
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "heading.2.markdown entity.name.section.markdown, heading.2.markdown punctuation.definition.heading.markdown",
        "settings": {
          "foreground": "#27995a",
          "fontStyle": "bold"
        }
      },
      {
        "scope": "heading.3.markdown entity.name.section.markdown, heading.3.markdown punctuation.definition.heading.markdown",
        "settings": {
          "foreground": "#cfd483"
        }
      },
      {
        "scope": "heading.4.markdown entity.name.section.markdown, heading.4.markdown punctuation.definition.heading.markdown",
        "settings": {
          "foreground": "#83d4d0"
        }
      },
      {
        "scope": "heading.5.markdown entity.name.section.markdown, heading.5.markdown punctuation.definition.heading.markdown",
        "settings": {
          "foreground": "#cf8b25"
        }
      }
    ]
  },
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.scrollback": 100000,
  "githubPullRequests.pullBranch": "never",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python",
    "editor.formatOnType": true
  },
  "redhat.telemetry.enabled": false,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "window.commandCenter": true,
  "window.zoomLevel": 1,
  "editor.minimap.enabled": false,
  "diffEditor.maxComputationTime": 0,
  "files.trimTrailingWhitespace": true,
  "files.trimFinalNewlines": true,
  "files.insertFinalNewline": true,
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml"
  },
  "workbench.editor.tabActionCloseVisibility": false,
  "editor.formatOnSave": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "github.copilot.editor.enableAutoCompletions": true,
  "editor.rulers": [
    80
  ]
}
